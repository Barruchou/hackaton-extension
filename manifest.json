
{
  "manifest_version": 2,
  "name": "Neos",
  "description": "Neos alpha!",
  "version": "1.0",
  "chrome_url_overrides" : {
    "newtab" : "template/index.html"
  },
  "browser_action": {
      "default_title": "Bienvenue",
      "browser_style": true
  },
  "permissions" : [
    "tabs",
    "activeTab"
  ],
  "background" : {
    "scripts" : ["./assets/js/background.js"]
  },
  "web_accessible_resources": ["./assets/img/avatar.png"]
}